<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_notification_log"
        style="@style/pretendard_medium_16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="@{@string/dynamic_text(item_notification_log_title})"
        tools:text="-> ㅇㅇ이가 들어왔어요." />

    <TextView
        style="@style/pretendart_regular_14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/gray"
        app:layout_constraintTop_toBottomOf="@id/tv_notification_log"
        app:layout_constraintStart_toStartOf="@id/tv_notification_log"
        android:layout_marginStart="19dp"
        tools:text="2024-07-07 14:30"
        />


</androidx.constraintlayout.widget.ConstraintLayout>
